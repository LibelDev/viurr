{
  "name": "viurr",
  "version": "1.3.0",
  "description": "Download programmes from Viu",
  "main": "./dist/index.js",
  "author": "kitce",
  "license": "MIT",
  "bin": "./dist/cli/cli.js",
  "engines": {
    "node": ">=12.13.0",
    "yarn": ">=1.9.1"
  },
  "scripts": {
    "clean:build": "rm -rf dist",
    "prebuild": "yarn clean:build",
    "cli": "ts-node src/cli/cli.ts",
    "build": "tsc",
    "lint": "eslint src/**",
    "clean:release": "rm -rf bin/viurr-*",
    "prerelease": "yarn clean:release && yarn build",
    "release": "pkg . --targets linux-x64,macos-x64,win-x64 --out-path bin",
    "prepublishOnly": "yarn build"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kitce/viurr"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "axios-extensions": "^3.0.3",
    "debug": "^4.1.1",
    "filenamify": "^4.1.0",
    "filesize": "^6.1.0",
    "jsdom": "^21.1.1",
    "lodash": "^4.17.10",
    "mime": "^2.3.1",
    "mkdirp": "^1.0.4",
    "mustache": "^2.3.0",
    "prettyjson": "^1.2.1",
    "progress": "^2.0.3",
    "uuid": "^9.0.0",
    "yargs": "^12.0.1"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/jsdom": "^21.1.1",
    "@types/lodash": "^4.14.112",
    "@types/mime": "^2.0.0",
    "@types/mkdirp": "^1.0.1",
    "@types/mustache": "^0.8.31",
    "@types/node": "^12.12.54",
    "@types/prettyjson": "^0.0.28",
    "@types/progress": "^2.0.3",
    "@types/uuid": "^9.0.1",
    "@types/yargs": "^11.1.0",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "eslint": "^7.6.0",
    "pkg": "^4.4.9",
    "ts-node": "^8.10.2",
    "typescript": "^5.0.4"
  }
}
